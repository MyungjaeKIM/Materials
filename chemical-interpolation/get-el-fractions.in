// Compute element fractions for the training and test set

// Load in a dataset of compounds
data = new data.materials.CompositionDataset
data import datasets\NaFeMnO_test_set.data

// Set to only compute element fractions
data attributes generators clear
data attributes generators add composition.ElementFractionAttributeGenerator

// Generate new attributes
timer start
data attributes generate
timer elapsed

// Set formation energy as property to be modeled
data target delta_e

// Save it in CSV format
save data datasets\NaFeMnO_test_set-fractions csv

// Do the same thing with the training set
data = data clone -empty
data import datasets\NaFeMnO_train_set.data

data attributes generate

data target delta_e
save data datasets\NaFeMnO_train_set-fractions csv

exit
